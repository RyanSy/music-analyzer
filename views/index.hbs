<h1>{{title}}</h1>

<form class="" action="/audiofeatures" method="post">
    <div class="form-group">
        <label class="form-label" for="trackName">Search by Artist and/or Title: </label>
        <input id="trackName" class="form-input" type="text" name="trackName" placeholder="Enter Artist and/or Title">
        <input type="hidden" name="grant_type" value="client_credentials">
    </div>
    <input class="btn btn-primary" type="submit" value="Analyze">
</form>
